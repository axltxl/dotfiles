{
    "title": "Launch apps",
    "rules": [
        {
            "description": "AppLaunch: Launch editor",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "w"
                            },
                            {
                                "key_code": "e"
                            }
                        ],
                        "modifiers": {
                            "mandatory": [
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Neovide.app'"
                        }
                    ]
                }
            ]
        },
        {
            "description": "AppLaunch: iTerm",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "w"
                            },
                            {
                                "key_code": "t"
                            }
                        ],
                        "modifiers": {
                            "mandatory": [
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/iTerm.app'"
                        }
                    ]
                }
            ]
        },
        {
            "description": "AppLaunch: Chrome",
            "manipulators": [
                {
                    "type": "basic",
                    "from": {
                        "simultaneous": [
                            {
                                "key_code": "w"
                            },
                            {
                                "key_code": "b"
                            }
                        ],
                        "modifiers": {
                            "mandatory": [
                                "option"
                            ]
                        }
                    },
                    "to": [
                        {
                            "shell_command": "open '/Applications/Google Chrome.app'"
                        }
                    ]
                }
            ]
        }
    ]
}
